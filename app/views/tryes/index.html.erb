<h1>Listing tryes</h1>

<table>
  <tr>
    <th>Team</th>
    <th>Code</th>
    <th>Match</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tryes.each do |trye| %>
  <tr>
    <td><%= trye.team_id %></td>
    <td><%= trye.code %></td>
    <td><%= trye.match %></td>
    <td><%= link_to 'Show', trye %></td>
    <td><%= link_to 'Edit', edit_trye_path(trye) %></td>
    <td><%= link_to 'Destroy', trye, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Trye', new_trye_path %>
